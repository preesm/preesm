Content-Type: text/ecl
Entry-Name: .content

get-view "Project Explorer" | get-link "Create a project..." | click
with [get-window "New Project"] {
    get-tree | select "Preesm/Preesm Project"
    get-button "Next >" | click -metaKeys ""
    get-editbox -after [get-label "Project name:"] | set-text simsdp
    get-button Finish | click -metaKeys ""
}
get-view "Project Explorer" | get-tree | select simsdp | get-menu -path "Preesm/Generate custom SimSDP Architecture" 
    | click -metaKeys ""
get-window "Generate a custom multinode architecture file." | get-editbox 
    -after [get-label "Enter the number of node."] | set-text 2
with [get-window -class CustomNodePropertiesDialog] {
    get-editbox -after [get-label "Enter the internode communication rate"] | set-text 200
    get-editbox -after [get-label "Enter the number of cores of node 0"] | set-text 1
    get-button "Configure Node 0" | click -metaKeys ""
    with [get-window -class CustomCorePropertiesDialog] {
        get-editbox -after [get-label "Enter the core frequency"] | set-text 2
        get-editbox -after [get-label "Enter the core communication rate"] | set-text 400
        get-combo -after [get-label "Enter the type of core"] | select x86
        get-button OK | click -metaKeys ""
    }
    get-editbox -after [get-label "Enter the number of cores of node 1"] | set-text 1
    get-button "Configure Node 1" | click -metaKeys ""
    with [get-window -class CustomCorePropertiesDialog] {
        get-editbox -after [get-label "Enter the core frequency"] | set-text 1
        get-editbox -after [get-label "Enter the core communication rate"] | set-text 200
        get-combo -after [get-label "Enter the type of core"] | select x86
        get-button OK | click -metaKeys ""
    }
    get-button OK | click -metaKeys ""
}
get-window "Generate a custom multinode architecture file." | get-button OK | click -metaKeys ""
get-window -class CustomNodePropertiesDialog | close
get-view "Project Explorer" | get-tree | select "simsdp/Archi/SimSDP_archi.csv" 
    | select "simsdp/Archi/SimSDP_archi.csv" | get-menu -path "Open With/Text Editor" | click -metaKeys ""
