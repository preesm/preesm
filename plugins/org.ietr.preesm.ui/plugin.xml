         point="org.eclipse.ui.newWizards">
      <wizard
            canFinishEarly="false"
            category="org.ietr.preesm.category"
            class="org.ietr.preesm.ui.NewGraphMLWizard"
            hasPages="true"
            icon="icons/graphiti.gif"
            id="org.ietr.preesm.ui.wizards.graphml"
            name="Algorithm (IBSDF)"
            project="false">
         <description>
            Create a new IBSDF algorithm.
         </description>
      </wizard>
      <wizard
            canFinishEarly="false"
            category="org.ietr.preesm.category"
            class="org.ietr.preesm.ui.NewIPXACTDesignWizard"
            hasPages="true"
            icon="icons/graphiti.gif"
            id="org.ietr.preesm.ui.wizards.ip-xact.design"
            name="Architecture Design (IP-XACT)"
            project="false">
         <description>
            Create a new architecture design.
         </description>
      </wizard>
      <wizard
            canFinishEarly="false"
