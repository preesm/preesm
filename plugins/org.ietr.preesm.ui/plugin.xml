            class="org.ietr.preesm.ui.console.CoreConsoleFactory"
            icon="icons/preesm2mini.PNG"
            label="Preesm Core Console">
      </consoleFactory>
   </extension>
   <extension
         id="albert"
         name="albert"
         point="org.eclipse.ui.ide.resourceFilters">
      <filter
            pattern="*.layout"
            selected="true">
         <description>
            albert
         </description>
      </filter>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <commonFilter
            activeByDefault="true"
            class="org.ietr.preesm.ui.ProjectExplorerFilter"
            description="Filters the *.layout files"
            id="org.ietr.preesm.core.layoutfilefilter"
            name="LayoutFileFilter"
            visibleInUI="true">
      </commonFilter>
      <commonFilter
            activeByDefault="true"
            class="org.ietr.preesm.ui.pimm.filter.ProjectExplorerPiFilter"
            description="Filters the *.pi files"
            id="org.ietr.preesm.experiment.pifilefilter"
            name="PiFileFilter"
            visibleInUI="true">
      </commonFilter>
   </extension>
   <extension
         id="org.ietr.preesm.ui.projectexplorerviewer"
         name="org.ietr.preesm.ui.projectexplorerviewer"
         point="org.eclipse.ui.navigator.viewer">
      <viewerContentBinding
            viewerId="org.eclipse.ui.navigator.ProjectExplorer">
         <includes>
            <contentExtension
                  isRoot="true"
                  pattern="org.ietr.preesm.core.layoutfilefilter">
            </contentExtension>
            <contentExtension
                  isRoot="true"
                  pattern="org.eclipse.ui.navigator.resourceContent">
            </contentExtension>
         </includes>
      </viewerContentBinding>
      <viewerContentBinding
            viewerId="org.eclipse.ui.navigator.ProjectExplorer">
         <includes>
            <contentExtension
                  isRoot="true"
                  pattern="org.ietr.preesm.experiment.pifilefilter">
            </contentExtension>
            <contentExtension
                  isRoot="true"
                  pattern="org.eclipse.ui.navigator.resourceContent">
            </contentExtension>
         </includes>
      </viewerContentBinding>
   </extension>
