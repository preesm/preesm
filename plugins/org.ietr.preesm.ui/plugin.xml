            class="org.ietr.preesm.ui.NewIPXACTComponentWizard"
            hasPages="true"
            icon="icons/graphiti.gif"
            id="org.ietr.preesm.ui.wizards.ip-xact.component"
            name="Architecture Component (IP-XACT)"
            project="false">
         <description>
            Create a new architecture component.
         </description>
      </wizard>
      <category
            id="org.ietr.preesm.category"
            name="Preesm">
      </category>
      <wizard
            category="org.ietr.preesm.category"
            class="org.ietr.preesm.ui.wizards.NewPreesmProjectWizard"
            hasPages="true"
            icon="icons/preesmProject.png"
            id="org.ietr.preesm.core.wizards.NewPreesmProjectWizard"
            name="Preesm Project"
            project="true">
         <description>
            Create a new Preesm project
         </description>
      </wizard>
      <wizard
            category="org.ietr.preesm.category"
            class="org.ietr.preesm.ui.wizards.NewScenarioFileWizard"
            hasPages="true"
            icon="icons/preesm4mini.PNG"
            id="org.ietr.preesm.core.ui.wizards.NewScenarioFileWizard"
            name="Scenario"
            project="false">
         <description>
            Create a new scenario file
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.ide.projectNatureImages">
      <image
            icon="icons/preesmProjectNature.png"
            id="org.ietr.preesm.core.ui.wizards.projectNatureImage"
            natureId="org.ietr.preesm.core.ui.wizards.nature">
         <description>
            A Preesm project.
         </description>
      </image>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.ietr.preesm.ui.scenario.editor.ScenarioEditor"
            default="true"
            extensions="scenario"
            icon="icons/preesm4mini.PNG"
            id="org.ietr.preesm.ui.scenario.editor.ScenarioEditor"
            name="Preesm scenario editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.console.consoleFactories">
      <consoleFactory
            class="org.ietr.preesm.ui.console.CoreConsoleFactory"
            icon="icons/preesm2mini.PNG"
            label="Preesm Core Console">
      </consoleFactory>
   </extension>
   <extension
         id="albert"
         name="albert"
         point="org.eclipse.ui.ide.resourceFilters">
      <filter
            pattern="*.layout"
            selected="true">
         <description>
            albert
         </description>
      </filter>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <commonFilter
            activeByDefault="true"
            class="org.ietr.preesm.ui.ProjectExplorerFilter"
            description="Filters the *.layout files"
            id="org.ietr.preesm.core.layoutfilefilter"
            name="LayoutFileFilter"
            visibleInUI="true">
      </commonFilter>
   </extension>
   <extension
         id="org.ietr.preesm.ui.projectexplorerviewer"
         name="org.ietr.preesm.ui.projectexplorerviewer"
         point="org.eclipse.ui.navigator.viewer">
      <viewerContentBinding
            viewerId="org.eclipse.ui.navigator.ProjectExplorer">
         <includes>
            <contentExtension
                  isRoot="true"
                  pattern="org.ietr.preesm.core.layoutfilefilter">
            </contentExtension>
            <contentExtension
                  isRoot="true"
                  pattern="org.eclipse.ui.navigator.resourceContent">
            </contentExtension>
         </includes>
      </viewerContentBinding>
   </extension>
   <extension
         id="org.ietr.preesm.core.ui.wizards.nature"
         name="Preesm Project"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="org.ietr.preesm.ui.wizards.PreesmProjectNature">
         </run>
      </runtime>
   </extension>
   <extension
         point="org.ietr.dftools.graphiti.model.definition">
      <configuration
            extension="graphml"
            name="GraphML"
            type="text/xml"
            validator="org.ietr.preesm.ui.editor.graph.validators.SDFValidator">
         <graphType
               name="Dataflow Graph">
            <parameter
                  default="sdf"
                  name="kind"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="name"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="graph parameter"
                  type="java.util.List">
            </parameter>
            <parameter
                  name="graph variable"
                  type="java.util.Map">
            </parameter>
         </graphType>
         <vertexType
               name="Input port">
            <parameter
                  name="id"
                  type="java.lang.String">
            </parameter>
            <parameter
                  default="port"
                  name="kind"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="refinement"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="instance argument"
                  type="java.util.Map">
            </parameter>
            <color
                  blue="100"
                  green="60"
                  red="0">
            </color>
            <size
                  height="30"
                  width="30">
            </size>
            <shape
                  type="triangle">
            </shape>
         </vertexType>
         <vertexType
               name="Output port">
            <parameter
                  name="id"
                  type="java.lang.String">
            </parameter>
            <parameter
                  default="port"
                  name="kind"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="refinement"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="instance argument"
                  type="java.util.Map">
            </parameter>
            <color
                  blue="100"
                  green="0"
                  red="60">
            </color>
            <shape
                  type="triangle">
            </shape>
            <size
                  height="30"
                  width="30">
            </size>
         </vertexType>
         <vertexType
               name="Vertex">
            <parameter
                  name="id"
                  type="java.lang.String">
            </parameter>
            <parameter
                  default="vertex"
                  name="kind"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="instance argument"
                  type="java.util.Map">
            </parameter>
            <parameter
                  name="refinement"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="memory script"
                  type="java.lang.String">
            </parameter>
            <color
                  blue="17"
                  green="188"
                  red="238">
            </color>
            <size
                  height="40"
                  width="40">
            </size>
            <shape
                  type="roundedBox">
            </shape>
         </vertexType>
         <edgeType
               directed="true"
               name="Dataflow edge">
            <parameter
                  name="source port"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="target port"
                  type="java.lang.String">
            </parameter>
            <parameter
                  default="1"
                  name="source production"
                  type="java.lang.String">
            </parameter>
            <parameter
                  default="1"
                  name="target consumption"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="source port modifier"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="target port modifier"
                  type="java.lang.String">
            </parameter>
            <parameter
                  default="0"
                  name="delay"
                  type="java.lang.String">
            </parameter>
            <parameter
                  default="char"
                  name="data type"
                  type="java.lang.String">
            </parameter>
         </edgeType>
         <import>
            <xslt
                  name="xslt/org/ietr/preesm/sdf4j/input_sdf4j.xslt">
            </xslt>
         </import>
         <export>
            <xslt
                  name="xslt/org/ietr/preesm/sdf4j/output_sdf4j.xslt">
            </xslt>
         </export>
         <vertexType
               name="Broadcast">
            <parameter
                  name="id"
                  type="java.lang.String">
            </parameter>
            <parameter
                  default="Broadcast"
                  name="kind"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="refinement"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="instance argument"
                  type="java.util.Map">
            </parameter>
            <color
                  blue="10"
                  green="120"
                  red="0">
            </color>
            <size
                  height="30"
                  width="30">
            </size>
            <shape
                  type="circle">
            </shape>
         </vertexType>
         <vertexType
               name="fork">
            <parameter
                  name="id"
                  type="java.lang.String">
            </parameter>
            <parameter
                  default="fork"
                  name="kind"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="refinement"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="instance argument"
                  type="java.util.Map">
            </parameter>
            <color
                  blue="0"
                  green="10"
                  red="120">
            </color>
            <size
                  height="30"
                  width="30">
            </size>
            <shape
                  type="circle">
            </shape>
         </vertexType>
