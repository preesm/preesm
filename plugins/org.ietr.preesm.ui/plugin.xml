<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.ietr.dftools.graphiti.ui.editors.GraphEditor"
            contributorClass="org.ietr.dftools.graphiti.ui.editors.GraphActionBarContributor"
            default="true"
            extensions="graphml, design, component"
            icon="icons/graphiti.gif"
            id="org.ietr.preesm.ui.editor.graph"
            name="Preesm Network Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.ietr.dftools.graphiti.ui.properties.contributor">
         <propertyTab
               category="org.ietr.dftools.graphiti.ui.properties.category.graph"
               id="org.ietr.preesm.ui.properties.tab.sdf.graph.parameters"
               label="Graph Parameters">
         </propertyTab>
         <propertyTab
               category="org.ietr.dftools.graphiti.ui.properties.category.graph"
               id="org.ietr.preesm.ui.properties.tab.sdf.graph.variables"
               label="Graph Variable Declarations">
         </propertyTab>
         <propertyTab
               category="org.ietr.dftools.graphiti.ui.properties.category.vertex"
               id="org.ietr.preesm.ui.properties.tab.sdf.vertex.arguments"
               label="Instance Arguments">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="org.ietr.dftools.graphiti.ui.properties.contributor">
         <propertySection
               class="org.ietr.preesm.ui.properties.SDFGraphParametersListSection"
               filter="org.ietr.preesm.ui.properties.filters.SDFGraphFilter"
               id="org.ietr.preesm.ui.properties.section.sdf.graph.parameters"
               tab="org.ietr.preesm.ui.properties.tab.sdf.graph.parameters">
         </propertySection>
         <propertySection
               class="org.ietr.preesm.ui.properties.SDFGraphVariablesMapSection"
               filter="org.ietr.preesm.ui.properties.filters.SDFGraphFilter"
               id="org.ietr.preesm.ui.properties.section.sdf.graph.variables"
               tab="org.ietr.preesm.ui.properties.tab.sdf.graph.variables">
         </propertySection>
         <propertySection
               class="org.ietr.preesm.ui.properties.SDFVertexInstanceArgumentsMapSection"
               filter="org.ietr.preesm.ui.properties.filters.SDFVertexFilter"
               id="org.ietr.preesm.ui.properties.section.sdf.vertex.arguments"
               tab="org.ietr.preesm.ui.properties.tab.sdf.vertex.arguments">
         </propertySection>
      </propertySections>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchShortcuts">
      <shortcut
            class="org.ietr.dftools.ui.workflow.launch.WorkflowLaunchShortcut"
            icon="icons/workflow.png"
            id="org.ietr.dftools.ui.workflow.workflowLaunchShortcut"
            label="Preesm Workflow"
            modes="run,debug">
         <description
               description="Launch a Preesm workflow"
               mode="run">
         </description>
         <contextualLaunch>
            <enablement>
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
                  <iterate>
                     <and>
                        <test
                              forcePluginActivation="true"
                              property="net.sf.dftools.isWorkflow">
                        </test>
                     </and>
                  </iterate>
               </with>
            </enablement>
         </contextualLaunch>
      </shortcut>
   </extension>
