            <parameter
                  name="source port"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="target port"
                  type="java.lang.String">
            </parameter>
            <parameter
                  default="1"
                  name="source production"
                  type="java.lang.String">
            </parameter>
            <parameter
                  default="1"
                  name="target consumption"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="source port modifier"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="target port modifier"
                  type="java.lang.String">
            </parameter>
            <parameter
                  default="0"
                  name="delay"
                  type="java.lang.String">
            </parameter>
            <parameter
                  default="char"
                  name="data type"
                  type="java.lang.String">
            </parameter>
         </edgeType>
         <import>
            <xslt
                  name="xslt/org/ietr/preesm/sdf4j/input_sdf4j.xslt">
            </xslt>
         </import>
         <export>
            <xslt
                  name="xslt/org/ietr/preesm/sdf4j/output_sdf4j.xslt">
            </xslt>
         </export>
         <vertexType
               name="Broadcast">
            <parameter
                  name="id"
                  type="java.lang.String">
            </parameter>
            <parameter
                  default="Broadcast"
                  name="kind"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="refinement"
                  type="java.lang.String">
            </parameter>
            <parameter
                  name="instance argument"
                  type="java.util.Map">
            </parameter>
            <color
                  blue="10"
                  green="120"
                  red="0">
            </color>
            <size
                  height="30"
                  width="30">
            </size>
            <shape
                  type="circle">
            </shape>
