            id="org.ietr.preesm.experiment.ui.pimm.wizards.NewPiMMWizard"
            name="Preesm Algorithm (PiSDF)"
            project="true">
         <description>
            Create a new PiMM algorithm.
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.ide.projectNatureImages">
      <image
            icon="icons/preesmProjectNature.png"
            id="org.ietr.preesm.core.ui.wizards.projectNatureImage"
            natureId="org.ietr.preesm.core.ui.wizards.nature">
         <description>
            A Preesm project.
         </description>
      </image>
