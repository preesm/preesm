   <!-- Preesm Menus -->
   <extension
         point="org.eclipse.ui.menus">
      <!-- actions available in the project navigator (not PiMM specific) -->
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?before=additions">
         <menu
               icon="resources/icons/preesmLogo_16x16.png"
               id="org.preesm.ui.menu1"
               label="Preesm">
            <!-- run the selected workflow -->
            <command
                  commandId="org.preesm.ui.command.RunWorkflow"
                  icon="resources/icons/workflow.png"
                  label="Run Workflow"
                  style="push"
                  tooltip="Launch a Preesm Workflow">
               <visibleWhen
                     checkEnabled="true">
                  <with
                        variable="selection">
                     <and>
                        <iterate
                              ifEmpty="false"
                              operator="and">
                           <adapt
                                 type="org.eclipse.core.resources.IResource">
                              <test
                                    property="org.eclipse.core.resources.extension"
                                    value="workflow">
                              </test>
                           </adapt>
                        </iterate>
                        <count
                              value="1">
                        </count>
                     </and>
                  </with>
               </visibleWhen>
            </command>
         </menu>
      </menuContribution>
      <!-- actions available in the WorkflowEditor popup -->
      <menuContribution
            allPopups="false"
            locationURI="popup:net.sf.graphiti.GraphitiContextMenu?before=additions">
         <!-- Run the workflow from the editor -->
         <command
               commandId="org.preesm.ui.command.RunWorkflow"
               icon="resources/icons/workflow.png"
               label="Run Workflow"
               style="push"
               tooltip="Launch a Preesm Workflow">
				<visibleWhen checkEnabled="true">
					<with variable="activeEditorId">
						<equals value="net.sf.dftools.ui.WorkflowEditor"/>
					</with>
				</visibleWhen>
         </command>
      </menuContribution>
   </extension>

   <!-- Preesm commands (for menus) -->
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.preesm.ui.command.RunWorkflow"
            name="Run Workflow">
      </command>
   </extension>

   <!-- Preesm command handlers (for menus) -->
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.preesm.ui.popup.actions.WorkflowRunnerPopup"
            commandId="org.preesm.ui.command.RunWorkflow">
      </handler>
   </extension>

   <!-- ?? -->
   <extension
         id="org.preesm.ui.projectexplorerviewer"
         name="org.preesm.ui.projectexplorerviewer"
         point="org.eclipse.ui.navigator.viewer">
      <viewerContentBinding
