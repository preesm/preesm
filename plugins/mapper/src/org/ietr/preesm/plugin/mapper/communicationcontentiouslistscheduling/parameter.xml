<?xml version="1.0" encoding="UTF-8"?>
<design_parameter>
	<vendor>ietr.org</vendor>
	<library>test_lib</library>
	<name>design_parameters</name>
	<version>1.0</version>
	<timeConstraint>20</timeConstraint>
	<surfaceConstraint>500</surfaceConstraint>
	<components>
		<component type="processor">
			<name>microblaze</name>
			<clockPeriod>1</clockPeriod>
			<!-- unit: ns -->
			<dataWidth>32</dataWidth>
			<surface>100</surface>
		</component>
		<component type="bus">
			<name>opb_bus</name>
			<clockPeriod>1</clockPeriod>
			<!-- unit: ns -->
			<dataWidth>32</dataWidth>
			<averageClockCyclesPerTransfer>
				2
			</averageClockCyclesPerTransfer>
			<!-- unit: bus clock cycle -->
			<portNumber>8</portNumber>
			<surface>100</surface>
		</component>
		<component type="switch">
			<name>opb_crossbar</name>
			<clockPeriod>1</clockPeriod>
			<!-- unit: ns -->
			<dataWidth>32</dataWidth>
			<averageClockCyclesPerTransfer>
				2
			</averageClockCyclesPerTransfer>
			<!-- unit: network clock cycle -->
			<portNumber>8</portNumber>
			<surface>100</surface>
		</component>
	</components>
</design_parameter>
