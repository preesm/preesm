            {: setAttribute(d, "assignable", "Yes"); RESULT = appendChild(d, e); :}
    |   decl_variable_state_head:d EQUALS expression:e
            {: RESULT = appendChild(d, e); :}
    |   MUTABLE decl_variable_state_head:d EQUALS expression:e
