qid ::=
     Id:id qid_tail:t
     {:
        Element qid = createElement("QID");
        Element e = createElement("ID");
        setName(e, id);
        appendChild(qid, e);
        for (int i = 0; i < t.size(); i++) {
            e = createElement("ID");
            setName(e, (String)t.get(i));
            appendChild(qid, e);
        }
        RESULT = qid;
     :}
     ;

qid_tail ::=
        /* empty */  {: RESULT = new Vector(); :}
    |   DOT id:i qid_tail:t
            {:
                t.add(0, i);
                RESULT = t;
            :}
     ;

