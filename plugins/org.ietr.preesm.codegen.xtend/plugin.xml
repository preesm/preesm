<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="net.sf.dftools.workflow.tasks">
      <task
            id="org.ietr.preesm.codegen.xtend.task.CodegenTask"
            name="Xtend Codegen"
            type="org.ietr.preesm.codegen.xtend.task.CodegenTask">
         <inputs>
            <input
                  id="MemEx"
                  object="org.ietr.preesm.memory.exclusiongraph.MemoryExclusionGraph">
            </input>
            <input
                  id="architecture"
                  object="net.sf.dftools.architecture.slam.Design">
            </input>
            <input
                  id="DAG"
                  object="net.sf.dftools.algorithm.model.dag.DirectedAcyclicGraph">
            </input>
            <input
                  id="scenario"
                  object="org.ietr.preesm.core.scenario.PreesmScenario">
            </input>
         </inputs>
      </task>
   </extension>

</plugin>
