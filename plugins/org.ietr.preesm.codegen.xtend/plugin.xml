<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="printers" name="Printers" schema="schema/printers.exsd"/>
   <extension
         point="net.sf.dftools.workflow.tasks">
      <task
            id="org.ietr.preesm.codegen.xtend.task.CodegenTask"
            name="Xtend Codegen"
            type="org.ietr.preesm.codegen.xtend.task.CodegenTask">
         <inputs>
            <input
                  id="MemEx"
                  object="org.ietr.preesm.memory.exclusiongraph.MemoryExclusionGraph">
            </input>
            <input
                  id="architecture"
                  object="net.sf.dftools.architecture.slam.Design">
            </input>
            <input
                  id="DAG"
                  object="net.sf.dftools.algorithm.model.dag.DirectedAcyclicGraph">
            </input>
            <input
                  id="scenario"
                  object="org.ietr.preesm.core.scenario.PreesmScenario">
            </input>
         </inputs>
      </task>
   </extension>
   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated codegen -->
      <package
            uri="http://codegen/1.0"
            class="org.ietr.preesm.codegen.xtend.model.codegen.CodegenPackage"
            genModel="model/codegen.genmodel"/>
   </extension>
   <extension
         point="org.ietr.preesm.codegen.xtend.printers">
      <printers
            language="XML">
         <printer
               class="org.ietr.preesm.codegen.xtend.printer.XMLPrinter"
               extension=".xml">
            <core_type
                  type="x86">
            </core_type>
            <core_type
                  type="c6678">
            </core_type>
         </printer>
      </printers>
      <printers
            language="C">
         <printer
               class="org.ietr.preesm.codegen.xtend.printer.c.CPrinter"
               extension=".c">
            <core_type
                  type="x86">
            </core_type>
         </printer>
         <printer
               class="org.ietr.preesm.codegen.xtend.printer.c.C6678CPrinter"
               extension=".c">
            <core_type
                  type="c6678">
            </core_type>
         </printer>
      </printers>
      <printers
            language="InstrumentedC">
         <printer
               class="org.ietr.preesm.codegen.xtend.printer.c.instrumented.InstrumentedCPrinter"
               extension=".c">
            <core_type
                  type="x86">
            </core_type>
         </printer>
      </printers>
   </extension>
</plugin>
