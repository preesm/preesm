<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns">
    <key attr.name="arguments" for="node" id="arguments"/>
    <key attr.name="parameters" for="graph" id="parameters"/>
    <key attr.name="variables" for="graph" id="variables"/>
    <key attr.name="name" attr.type="string" for="graph"/>
    <key attr.name="model" attr.type="string" for="graph"/>
    <key attr.name="name" attr.type="string" for="node"/>
    <key attr.name="kind" attr.type="string" for="node"/>
    <key attr.name="port_direction" attr.type="string" for="node"/>
    <key attr.name="graph_desc" attr.type="string" for="node"/>
    <key attr.name="nbRepeat" attr.type="int" for="node"/>
    <key attr.name="edge_cons" attr.type="string" for="edge"/>
    <key attr.name="edge_delay" attr.type="string" for="edge"/>
    <key attr.name="edge_prod" attr.type="string" for="edge"/>
    <key attr.name="data_type" attr.type="string" for="edge"/>
    <graph edgedefault="directed">
        <data key="name">top_display</data>
        <data key="kind">sdf</data>
        <data key="parameters"/>
        <data key="variables">
            <variable name="N" value="8"/>
            <variable name="xsize" value="1920"/>
            <variable name="ysize" value="1080"/>
        </data>
        <node id="Read_YUV">
            <data key="name">Read_YUV</data>
            <data key="kind">vertex</data>
            <data key="graph_desc">../Code/IDL/readYuv.idl</data>
            <data key="arguments">
                <argument name="xSize" value="xsize"/>
                <argument name="ySize" value="ysize"/>
            </data>
        </node>
        <node id="display_pic">
            <data key="name">display_pic</data>
            <data key="kind">vertex</data>
            <data key="graph_desc">../Code/IDL/displayPic.idl</data>
            <data key="arguments">
                <argument name="index" value="0"/>
                <argument name="xsize" value="xsize"/>
                <argument name="ysize" value="ysize"/>
            </data>
        </node>
        <node id="SobelY">
            <data key="name">SobelY</data>
            <data key="kind">vertex</data>
            <data key="graph_desc">../Code/IDL/sobel.idl</data>
            <data key="arguments">
                <argument name="xSize" value="xsize"/>
                <argument name="ySize" value="ysize/N+2"/>
            </data>
        </node>
        <node id="display_res">
            <data key="name">display_res</data>
            <data key="kind">vertex</data>
            <data key="graph_desc">../Code/IDL/displayPic.idl</data>
            <data key="arguments">
                <argument name="index" value="1"/>
                <argument name="xsize" value="xsize"/>
                <argument name="ysize" value="ysize"/>
            </data>
        </node>
        <node id="Br_U">
            <data key="name">Br_U</data>
            <data key="kind">Broadcast</data>
            <data key="graph_desc"/>
            <data key="arguments"/>
        </node>
        <node id="Br_V">
            <data key="name">Br_V</data>
            <data key="kind">Broadcast</data>
            <data key="graph_desc"/>
            <data key="arguments"/>
        </node>
        <node id="Br_Y">
            <data key="name">Br_Y</data>
            <data key="kind">Broadcast</data>
            <data key="graph_desc"/>
            <data key="arguments"/>
        </node>
        <node id="SplitY">
            <data key="name">SplitY</data>
            <data key="kind">vertex</data>
            <data key="graph_desc">../Code/IDL/split.idl</data>
            <data key="arguments">
                <argument name="nbSlice" value="N"/>
                <argument name="xSize" value="xsize"/>
                <argument name="ySize" value="ysize"/>
            </data>
        </node>
        <node id="MergeY">
            <data key="name">MergeY</data>
            <data key="kind">vertex</data>
            <data key="graph_desc">../Code/IDL/merge.idl</data>
            <data key="arguments">
                <argument name="nbSlice" value="N"/>
                <argument name="xSize" value="xsize"/>
                <argument name="ySize" value="ysize"/>
            </data>
        </node>
        <edge source="Read_YUV" sourceport="u" target="Br_U" targetport="u">
            <data key="edge_prod">xsize/2*ysize/2</data>
            <data key="edge_delay">xsize/2*ysize/2</data>
            <data key="edge_cons">xsize/2*ysize/2</data>
            <data key="data_type">uchar</data>
        </edge>
        <edge source="Br_U" sourceport="u" target="display_res" targetport="u">
            <data key="edge_prod">xsize/2*ysize/2</data>
            <data key="edge_delay">xsize/2*ysize/2</data>
            <data key="edge_cons">xsize/2*ysize/2</data>
            <data key="data_type">uchar</data>
        </edge>
        <edge source="Br_U" sourceport="u" target="display_pic" targetport="u">
            <data key="edge_prod">xsize/2*ysize/2</data>
            <data key="edge_delay">xsize/2*ysize/2</data>
            <data key="edge_cons">xsize/2*ysize/2</data>
            <data key="data_type">uchar</data>
        </edge>
        <edge source="Read_YUV" sourceport="v" target="Br_V" targetport="v">
            <data key="edge_prod">xsize/2*ysize/2</data>
            <data key="edge_delay">xsize/2*ysize/2</data>
            <data key="edge_cons">xsize/2*ysize/2</data>
            <data key="data_type">uchar</data>
        </edge>
        <edge source="Br_V" sourceport="v" target="display_res" targetport="v">
            <data key="edge_prod">xsize/2*ysize/2</data>
            <data key="edge_delay">xsize/2*ysize/2</data>
            <data key="edge_cons">xsize/2*ysize/2</data>
            <data key="data_type">uchar</data>
        </edge>
        <edge source="Read_YUV" sourceport="y" target="Br_Y" targetport="y">
            <data key="edge_prod">xsize*ysize</data>
            <data key="edge_delay">xsize*ysize</data>
            <data key="edge_cons">xsize*ysize</data>
            <data key="data_type">uchar</data>
        </edge>
        <edge source="Br_V" sourceport="v" target="display_pic" targetport="v">
            <data key="edge_prod">xsize/2*ysize/2</data>
            <data key="edge_delay">xsize/2*ysize/2</data>
            <data key="edge_cons">xsize/2*ysize/2</data>
            <data key="data_type">uchar</data>
        </edge>
        <edge source="Br_Y" sourceport="y" target="SplitY" targetport="input">
            <data key="edge_prod">xsize*ysize</data>
            <data key="edge_delay">0</data>
            <data key="edge_cons">xsize*ysize</data>
            <data key="data_type">uchar</data>
        </edge>
        <edge source="SplitY" sourceport="output" target="SobelY" targetport="input">
            <data key="edge_prod">xsize*ysize+2*N*xsize</data>
            <data key="edge_delay">0</data>
            <data key="edge_cons">xsize*ysize/N+2*xsize</data>
            <data key="data_type">uchar</data>
        </edge>
        <edge source="SobelY" sourceport="output" target="MergeY" targetport="input">
            <data key="edge_prod">xsize*ysize/N+2*xsize</data>
            <data key="edge_delay">0</data>
            <data key="edge_cons">xsize*ysize+2*N*xsize</data>
            <data key="data_type">uchar</data>
        </edge>
        <edge source="MergeY" sourceport="output" target="display_res" targetport="y">
            <data key="edge_prod">xsize*ysize</data>
            <data key="edge_delay">xsize*ysize</data>
            <data key="edge_cons">xsize*ysize</data>
            <data key="data_type">uchar</data>
        </edge>
        <edge source="Br_Y" sourceport="y" target="display_pic" targetport="y">
            <data key="edge_prod">xsize*ysize</data>
            <data key="edge_delay">xsize*ysize</data>
            <data key="edge_cons">xsize*ysize</data>
            <data key="data_type">uchar</data>
        </edge>
    </graph>
</graphml>
